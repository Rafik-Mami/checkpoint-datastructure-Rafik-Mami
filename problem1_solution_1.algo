ALGORITHM algorithm_SUM
VAR
htab:HASH_TABLE<INTEGER,INTEGER>
i,j,SUM:INTEGER;
str1:STRING[10]
str2:STRING[10]


BEGIN
read(str1);
read(str2);
FOR i FROM 0 TO set1.LENGTH-1 STEP 1  DO
X:BOOLEAN:=FALSE ;
J:=0;
WHILE(x)
if (str1[i]==str2[j])
X:=TRUE
j++
END_WHILE
IF X:=FALSE
SUM +=str1[i]
END_FOR

FOR i FROM 0 TO set2.LENGTH-1 STEP 1  DO
X:BOOLEAN:=FALSE ;
